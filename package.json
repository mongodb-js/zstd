  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@mongodb-js/zstd": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "chai": "^4.5.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "mocha": "^10.8.2",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">= 16.20.1"
  },
  "scripts": {
    "compile": "node-gyp rebuild",
